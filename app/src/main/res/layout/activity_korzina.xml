<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Korzina">

    <TextView
        android:id="@+id/list2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:text="@string/list_flights"
        android:textAlignment="center"
        android:textColor="@color/material_dynamic_tertiary10"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TableLayout
        android:id="@+id/dbOutput"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/nazad3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/list2"></TableLayout>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/nazad3"
        android:layout_width="122dp"
        android:layout_height="51dp"
        android:layout_marginStart="17dp"
        android:layout_marginEnd="118dp"
        android:layout_marginBottom="16dp"
        android:layout_weight="1"
        android:background="@drawable/style"
        android:padding="5dp"
        android:text="Назад"
        android:textColor="@color/material_dynamic_tertiary10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/itog"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/itog"
        android:layout_width="122dp"
        android:layout_height="51dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="16dp"
        android:layout_weight="1"
        android:background="@drawable/style"
        android:padding="5dp"
        android:text="Итог"
        android:textColor="@color/material_dynamic_tertiary10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.887"
        app:layout_constraintStart_toEndOf="@+id/nazad3" />
</androidx.constraintlayout.widget.ConstraintLayout>